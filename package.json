{
	"name": "foodmap",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"pnpm": {
		"overrides": {
			"sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.0",
			"source-map@^0.8.0-beta.0": "^0.7.4"
		}
	},
	"scripts": {
		"dev": "npm run parse:restaurants && vite dev",
		"build": "npm run parse:restaurants && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"parse:restaurants": "node scripts/parse-restaurants.js"
	},
	"devDependencies": {
		"@picocss/pico": "^2.1.1",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.50.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@vite-pwa/sveltekit": "^1.1.0",
		"maplibre-gl": "^4.7.1",
		"svelte": "^5.49.1",
		"svelte-check": "^4.3.6",
		"svelte-maplibre-gl": "^1.0.3",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vite-plugin-pwa": "^1.2.0",
		"yaml": "^2.8.2"
	}
}
